 1957|       |            let node = BlankNode::new_unchecked(id);
 1958|       |            if state.used_bnodes.contains(&node) {
 1959|       |                Err("Already used blank node id")
 1960|       |            } else {
 1961|       |                state.currently_used_bnodes.insert(node.clone());
